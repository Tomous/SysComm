#import <Foundation/Foundation.h>

@interface DCPersonalModel : NSObject
/**
 *  email，座机，手机号
 */
@property(nonatomic,copy)NSString *email;
@property(nonatomic,copy)NSString *mobile;
@property(nonatomic,copy)NSString *telphone;
/**
 *  名字
 */
@property(nonatomic,copy)NSString *relname;
@property(nonatomic,copy)NSString *pinyin;
/**
 *  微信号，QQ号，
 */
@property(nonatomic,copy)NSString *weixin;
@property(nonatomic,copy)NSString *qq;
/**
 *  性别
 */
@property(nonatomic,copy)NSString *sex;
/**
 *  用户头像,刚进来的时候是空的
 */
@property(nonatomic,copy)NSString *photos;

/**
 *  用户所在城市
 */
@property(nonatomic,copy)NSString *lastgeo;

@property(nonatomic,copy)NSString *url;
@property(nonatomic,strong)NSDictionary *group_job;

+(DCPersonalModel *)sharePersonModel;
@end
